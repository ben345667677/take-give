{
  "name": "marketplace-tests",
  "version": "1.0.0",
  "description": "API Integration Tests for Trade & Give Marketplace",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "testEnvironment": "node",
    "testTimeout": 30000,
    "verbose": true,
    "collectCoverageFrom": [
      "integration/**/*.js",
      "!**/node_modules/**"
    ]
  },
  "keywords": [
    "tests",
    "api",
    "integration"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.2",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  }
}
